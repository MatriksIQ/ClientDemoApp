    <!--LINK STYLE CHECKBOX-->
    <Style x:Key="CheckboxLink" TargetType="{x:Type CheckBox}">
        <Setter Property="SnapsToDevicePixels" Value="true"/>
        <Setter Property="Cursor" Value="Hand"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="CheckBox">
                    <BulletDecorator Background="Transparent">
                        <ContentPresenter Margin="1,0,0,0" HorizontalAlignment="Left" RecognizesAccessKey="True"/>
                        <BulletDecorator.Bullet>
                            <Border x:Name="Border" CornerRadius="0" Background="Transparent" BorderThickness="0" BorderBrush="Transparent">
                                <Canvas Width="13" Height="13">
                                    <Path Fill="{TemplateBinding Foreground}" Width="13" Height="13" Name="Checked1" Stretch="Uniform" Data="F 1 M 18.177,6.899 C 18.49,6.919 18.594,7.142 18.408,7.395 L 10.658,17.914 C 10.472,18.166 10.174,18.162 9.995,17.904 L 6.628,13.072 C 6.448,12.815 6.458,12.401 6.649,12.152 L 6.734,12.04 C 6.924,11.791 7.271,11.759 7.505,11.969 L 9.811,14.044 C 10.043,14.254 10.401,14.231 10.605,13.992 L 16.425,7.737 C 16.638,7.507 17.029,7.205 17.293,7.064 17.558,6.925 18.177,6.899 18.177,6.899"/>
                                    <Path Fill="{TemplateBinding Foreground}" Width="13" Height="13" Name="Checked2" Data="F 1 M 11.227,3.589 L 11.227,12.372 0.5,12.372 0.5,1.647 10.07,1.647 10.535,1.147 0,1.147 0,12.872 11.727,12.872 11.727,2.911 Z"/>
                                    <Path Fill="{TemplateBinding Foreground}" Visibility="Collapsed" Name="Unchecked" Data="F 1 M 11.727,12.872 L 0,12.872 0,1.148 11.727,1.148 11.727,12.872 Z M 0.5,12.372 L 11.227,12.372 11.227,1.648 0.5,1.648 0.5,12.372 Z"/>
                                </Canvas>
                            </Border>
                        </BulletDecorator.Bullet>
                    </BulletDecorator>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsChecked" Value="false">
                            <Setter Property="Foreground" Value="{DynamicResource Link.Checkbox.Static.Foreground}"/>
                            <Setter TargetName="Checked1" Property="Visibility" Value="Collapsed"/>
                            <Setter TargetName="Checked2" Property="Visibility" Value="Collapsed"/>
                            <Setter TargetName="Unchecked" Property="Visibility" Value="Visible"/>
                        </Trigger>
                        <Trigger Property="IsChecked" Value="{x:Null}">
                            <Setter Property="Foreground" Value="{DynamicResource Link.Checkbox.Checked.Foreground}"/>
                            <Setter TargetName="Checked1" Property="Visibility" Value="Collapsed"/>
                            <Setter TargetName="Checked2" Property="Visibility" Value="Collapsed"/>
                            <Setter TargetName="Unchecked" Property="Visibility" Value="Visible"/>
                        </Trigger>
                        <Trigger Property="IsChecked" Value="True">
                            <Setter Property="Foreground" Value="{DynamicResource Link.Checkbox.Checked.Foreground}"/>
                            <Setter TargetName="Checked1" Property="Visibility" Value="Visible"/>
                            <Setter TargetName="Checked2" Property="Visibility" Value="Visible"/>
                            <Setter TargetName="Unchecked" Property="Visibility" Value="Collapsed"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="true">
                            <Setter Property="Foreground" Value="{DynamicResource Link.Checkbox.Pressed.Foreground}"/>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter Property="IsChecked" Value="False"/>
                        </Trigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsMouseOver" Value="true"/>
                                <Condition Property="IsChecked" Value="false" />
                            </MultiTrigger.Conditions>
                            <MultiTrigger.Setters>
                                <Setter TargetName="Checked1" Property="Visibility" Value="Visible"/>
                                <Setter TargetName="Checked2" Property="Visibility" Value="Visible"/>
                                <Setter TargetName="Checked1" Property="Fill" Value="{DynamicResource Link.Checkbox.MouseOver.Foreground}"/>
                                <Setter TargetName="Unchecked" Property="Visibility" Value="Collapsed"/>
                            </MultiTrigger.Setters>
                        </MultiTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>


